{
  "name": "youtube-to-tana",
  "title": "YouTube to Tana",
  "description": "Extract YouTube video information and format it for Tana",
  "icon": "command-icon.png",
  "author": "Lisa Ross",
  "categories": [
    "Productivity",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Extract YouTube Video",
      "description": "Extract information from current YouTube video and format for Tana",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.0",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write ."
  }
} 